---
const partners: string[] = [
  'aguara',
  'transportemostto',
  'soychu',
  'rontaltex',
  'noelma',
  'mitreconstrucciones',
  'inprocil',
  'imperialcord',
  'hornus',
  'hermann',
  'entrerioscrushing',
  'elrodeo',
  'construccionesdeloeste',
  'codegu',
  'cappeletti',
  'bonnin',
  'baggio',
  'alexisneuwirt',
